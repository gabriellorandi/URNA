<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="442.0" prefWidth="809.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Cargo.CargoController">
   <children>
      <TextField fx:id="txtNome" layoutX="22.0" layoutY="30.0" prefHeight="25.0" prefWidth="193.0" promptText="Nome" AnchorPane.leftAnchor="22.0" AnchorPane.rightAnchor="594.0" AnchorPane.topAnchor="30.0" />
      <Button fx:id="btnCadastrar" layoutX="650.0" layoutY="30.0" mnemonicParsing="false" onAction="#cadastrarCargo" prefHeight="25.0" prefWidth="139.0" text="Adicionar" />
      <Button fx:id="btnCancel" cancelButton="true" layoutX="643.0" layoutY="382.0" mnemonicParsing="false" onAction="#close" prefHeight="25.0" prefWidth="139.0" text="Cancelar" />
      <TableView fx:id="tableView" layoutX="22.0" layoutY="72.0" prefHeight="284.0" prefWidth="760.0">
         <columns>
            <TableColumn fx:id="cargoId" prefWidth="175.0" text="ID" />
            <TableColumn fx:id="cargoNome" prefWidth="416.0" text="Nome" />
         </columns>
      </TableView>
      <Button fx:id="btnRemove" layoutX="22.0" layoutY="370.0" mnemonicParsing="false" onAction="#removerCargo" prefHeight="25.0" prefWidth="146.0" text="Remover" />
   </children>
</AnchorPane>
