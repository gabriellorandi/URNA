<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="507.0" prefWidth="809.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Eleicao.EleicaoController">
   <children>
      <Button fx:id="btnCadastrar" layoutX="486.0" layoutY="30.0" mnemonicParsing="false" onAction="#cadastrarEleicao" prefHeight="25.0" prefWidth="139.0" text="Adicionar" />
      <Button fx:id="btnDeslogar" cancelButton="true" layoutX="318.0" layoutY="456.0" mnemonicParsing="false" onAction="#deslogar" prefHeight="25.0" prefWidth="139.0" text="Deslogar" />
      <TableView fx:id="tableView" layoutX="22.0" layoutY="72.0" prefHeight="284.0" prefWidth="760.0">
         <columns>
            <TableColumn fx:id="eleicaoId" prefWidth="120.0" text="ID" />
            <TableColumn fx:id="eleicaoDia" prefWidth="639.0" text="Dia" />
         </columns>
      </TableView>
      <Button fx:id="btnSecao" cancelButton="true" layoutX="179.0" layoutY="382.0" mnemonicParsing="false" onAction="#abrirSecao" prefHeight="25.0" prefWidth="139.0" text="Seção" />
      <DatePicker fx:id="dateEleicao" layoutX="22.0" layoutY="30.0" />
      <Button fx:id="btnRemover" cancelButton="true" layoutX="643.0" layoutY="30.0" mnemonicParsing="false" onAction="#removerEleicao" prefHeight="25.0" prefWidth="139.0" text="Remover" />
      <Button fx:id="btnEleitores" cancelButton="true" layoutX="335.0" layoutY="382.0" mnemonicParsing="false" onAction="#abrirEleitores" prefHeight="25.0" prefWidth="139.0" text="Eleitores" />
      <Button fx:id="btnMesario" cancelButton="true" layoutX="22.0" layoutY="382.0" mnemonicParsing="false" onAction="#abrirMesario" prefHeight="25.0" prefWidth="139.0" text="Mesário" />
      <Button fx:id="btnCandidato" cancelButton="true" layoutX="497.0" layoutY="382.0" mnemonicParsing="false" onAction="#abrirCandidato" prefHeight="25.0" prefWidth="139.0" text="Candidatos" />
   </children>
</AnchorPane>
