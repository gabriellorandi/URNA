<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import org.controlsfx.control.CheckComboBox?>

<AnchorPane prefHeight="442.0" prefWidth="809.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Eleicao.EleicaoController">
   <children>
      <Button fx:id="btnCadastrar" layoutX="650.0" layoutY="30.0" mnemonicParsing="false" onAction="#cadastrarEleicao" prefHeight="25.0" prefWidth="139.0" text="Adicionar" />
      <Button fx:id="btnCancel" cancelButton="true" layoutX="650.0" layoutY="382.0" mnemonicParsing="false" onAction="#close" prefHeight="25.0" prefWidth="139.0" text="Cancelar" />
      <TableView fx:id="tableView" layoutX="22.0" layoutY="72.0" prefHeight="284.0" prefWidth="760.0">
         <columns>
            <TableColumn fx:id="eleicaoId" prefWidth="135.0" text="ID" />
            <TableColumn fx:id="eleicaoDia" prefWidth="244.0" text="Dia" />
            <TableColumn fx:id="eleicaoCandidato" prefWidth="176.0" text="Candidatos" />
            <TableColumn fx:id="eleicaoSecoes" prefWidth="200.0" text="Seções" />
         </columns>
      </TableView>
      <Button fx:id="btnSecao" cancelButton="true" layoutX="22.0" layoutY="382.0" mnemonicParsing="false" onAction="#abrirSecao" prefHeight="25.0" prefWidth="139.0" text="Seção" />
      <CheckComboBox fx:id="ccbCandidatos" layoutX="228.0" layoutY="30.0" prefHeight="25.0" prefWidth="162.0" />
      <DatePicker fx:id="dateEleicao" layoutX="22.0" layoutY="30.0" />
      <Button fx:id="btnRemover" cancelButton="true" layoutX="196.0" layoutY="382.0" mnemonicParsing="false" onAction="#removerEleicao" prefHeight="25.0" prefWidth="139.0" text="Remover" />
   </children>
</AnchorPane>
